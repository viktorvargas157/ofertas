<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OcJobs" table="oc_jobs">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="class" type="string" column="class" length="255" nullable="false"/>
    <field name="argument" type="string" column="argument" length="4000" nullable="false"/>
    <field name="lastRun" type="integer" column="last_run" nullable="true"/>
    <field name="lastChecked" type="integer" column="last_checked" nullable="true"/>
    <field name="reservedAt" type="integer" column="reserved_at" nullable="true"/>
    <field name="executionDuration" type="integer" column="execution_duration" nullable="false"/>
  </entity>
</doctrine-mapping>
