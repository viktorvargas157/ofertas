<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OcStorages" table="oc_storages">
    <id name="numericId" type="integer" column="numeric_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="id" type="string" column="id" length="64" nullable="true"/>
    <field name="available" type="integer" column="available" nullable="false"/>
    <field name="lastChecked" type="integer" column="last_checked" nullable="true"/>
  </entity>
</doctrine-mapping>
