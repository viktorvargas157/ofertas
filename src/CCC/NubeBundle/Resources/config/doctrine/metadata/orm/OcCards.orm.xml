<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OcCards" table="oc_cards">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="addressbookid" type="integer" column="addressbookid" nullable="false"/>
    <field name="carddata" type="blob" column="carddata" nullable="true"/>
    <field name="uri" type="string" column="uri" length="255" nullable="true"/>
    <field name="lastmodified" type="bigint" column="lastmodified" nullable="true"/>
    <field name="etag" type="string" column="etag" length="32" nullable="true"/>
    <field name="size" type="bigint" column="size" nullable="false"/>
  </entity>
</doctrine-mapping>
