<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OcShareExternal" table="oc_share_external">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="remote" type="string" column="remote" length="512" nullable="false"/>
    <field name="remoteId" type="bigint" column="remote_id" nullable="false"/>
    <field name="shareToken" type="string" column="share_token" length="64" nullable="false"/>
    <field name="password" type="string" column="password" length="64" nullable="true"/>
    <field name="name" type="string" column="name" length="64" nullable="false"/>
    <field name="owner" type="string" column="owner" length="64" nullable="false"/>
    <field name="user" type="string" column="user" length="64" nullable="false"/>
    <field name="mountpoint" type="string" column="mountpoint" length="4000" nullable="false"/>
    <field name="mountpointHash" type="string" column="mountpoint_hash" length="32" nullable="false"/>
    <field name="accepted" type="integer" column="accepted" nullable="false"/>
  </entity>
</doctrine-mapping>
