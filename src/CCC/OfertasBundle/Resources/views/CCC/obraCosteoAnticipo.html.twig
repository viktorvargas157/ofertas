                                                                        {% set anticipoMesAlimentacion = 0 %}
                                                                        {% set anticipoMesTransp = 0 %}
                                                                        {% for sociosDO in costeo.sociosDO %}
                                                                            {% set anticipoMes = anticipoMes + sociosDO.anticipo %}
                                                                            {% set anticipoMesAlimentacion = anticipoMesAlimentacion + sociosDO.alimentacion %}
                                                                            {% set anticipoMesTransp = anticipoMesTransp + sociosDO.transporte %}
                                                                        {% endfor %}
                                                                        <div class="row">
                                                                            <div class="col-lg-12">
                                                                                <section class="panel">
                                                                                    <table class="table table-bordered">
                                                                                        <thead>
                                                                                        <tr style="text-align: center">
                                                                                            <th style="text-align: center"><i class="icon_cogs" style="text-align: center"></i></th>
                                                                                            <th style="text-align: center">Total Anticipo Salario</th>
                                                                                            <th style="text-align: center">Total Anticipo Alimentación</th>
                                                                                            <th style="text-align: center">Total Anticipo Transporte</th>
                                                                                        </tr>
                                                                                        </thead>
                                                                                        <tbody>
                                                                                        <tr>
                                                                                            <td style="text-align: center"><strong>Quincenal</strong></td>
                                                                                            <td style="text-align: center">$ {{ (anticipoMes / 2)|number_format(2, '.', ',') }}</td>
                                                                                            <td style="text-align: center">$ {{ (anticipoMesAlimentacion / 2)|number_format(2, '.', ',') }}</td>
                                                                                            <td style="text-align: center">$ {{ (anticipoMesTransp / 2  )|number_format(2, '.', ',') }}</td>
                                                                                        </tr>
                                                                                        <tr>
                                                                                            <td style="text-align: center"><strong>Mensual</strong></td>
                                                                                            <td style="text-align: center">$ {{ anticipoMes|number_format(2, '.', ',') }}</td>
                                                                                            <td style="text-align: center">$ {{ anticipoMesAlimentacion |number_format(2, '.', ',') }}</td>
                                                                                            <td style="text-align: center">$ {{ anticipoMesTransp |number_format(2, '.', ',') }}</td>
                                                                                        </tr>
                                                                                        </tbody>
                                                                                    </table>
                                                                                </section>
                                                                            </div>
                                                                        </div>

